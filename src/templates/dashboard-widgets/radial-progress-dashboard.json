{
  "templateId": "tmpl-dashboard-radial-010",
  "name": "Radial Progress Dashboard",
  "description": "Stunning circular gauge dashboard with animated progress rings. Uses SVG-based radial progress bars to visualize completion percentages. Perfect for goals, quotas, and target tracking.",
  "category": "dashboard-widgets",
  "thumbnailUrl": "/templates/thumbnails/radial-progress.png",
  "previewHtml": "<div class='radial-dashboard'><h2>Q4 Goals Progress</h2><div class='radials'>[Circular Progress: 87%] [Circular Progress: 65%] [Circular Progress: 92%]</div></div>",
