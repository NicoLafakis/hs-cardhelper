                "props": {
                  "value": 87,
                  "max": 100,
                  "size": 160,
                  "strokeWidth": 12,
                  "color": "#10b981",
                  "trackColor": "rgba(255,255,255,0.2)",
                  "label": "Revenue Target",
                  "sublabel": "$437K / $500K",
                  "animated": true,
                  "duration": 1500,
                  "easing": "easeOutCubic"
                }
              },
              {
                "type": "CustomComponent",
                "component": "RadialProgress",
                "props": {
                  "value": 65,
                  "max": 100,
                  "size": 160,
                  "strokeWidth": 12,
                  "color": "#f59e0b",
                  "trackColor": "rgba(255,255,255,0.2)",
                  "label": "New Customers",
                  "sublabel": "325 / 500",
                  "animated": true,
                  "duration": 1500,
                  "easing": "easeOutCubic"
                }
              },
              {
                "type": "CustomComponent",
                "component": "RadialProgress",
                "props": {
                  "value": 92,
                  "max": 100,
                  "size": 160,
                  "strokeWidth": 12,
                  "color": "#06b6d4",
                  "trackColor": "rgba(255,255,255,0.2)",
                  "label": "Customer Satisfaction",
                  "sublabel": "4.6 / 5.0",
                  "animated": true,
                  "duration": 1500,
                  "easing": "easeOutCubic"
                }
              }
            ]
          },
          {
            "type": "Stack",
            "props": { "spacing": "12px", "direction": "horizontal", "justify": "center", "align": "center" },
            "children": [
              {
                "type": "Text",
                "props": {
                  "value": "Overall Progress:",
                  "fontSize": "16px",
                  "fontWeight": "500",
                  "color": "rgba(255,255,255,0.9)"
                }
              },
              {
                "type": "Text",
                "props": {
                  "value": "81.3%",
                  "fontSize": "28px",
                  "fontWeight": "700",
                  "color": "#ffffff"
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "implementationNotes": {
    "chartLibrary": "Custom SVG or react-circular-progressbar",
    "installation": "npm install react-circular-progressbar",
    "codeExample": "import { CircularProgressbarWithChildren, buildStyles } from 'react-circular-progressbar';\nimport 'react-circular-progressbar/dist/styles.css';\n\n<CircularProgressbarWithChildren\n  value={87}\n  styles={buildStyles({\n    pathColor: '#10b981',\n    trailColor: 'rgba(255,255,255,0.2)',\n    pathTransitionDuration: 1.5\n  })}\n>\n  <div style={{ fontSize: 32, fontWeight: 700 }}>87%</div>\n  <div style={{ fontSize: 14, marginTop: -5 }}>Revenue</div>\n</CircularProgressbarWithChildren>",
    "features": [
      "Animated circular progress bars with easing",
      "Customizable colors and stroke widths",
      "Center labels with value and description",
      "Responsive sizing for different viewports",
      "Gradient background for premium look",
      "SVG-based for crisp rendering at any size"
    ]
  },
  "designTokens": {
